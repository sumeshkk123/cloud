    },
    "highlights": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.highlight-card"
    },
    "cards": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.mega-card"
    },
    "serviceList": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.link-with-icon"
    },
    "serviceDetails": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.mega-detail"
    },
    "categories": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.mega-category"
    },
    "callouts": {
      "type": "component",
      "repeatable": true,
      "component": "navigation.mega-callout"
    },
    "seeAll": {
      "type": "component",
      "component": "shared.link"
    }
  }
